<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <context id="HospitalDB4" targetRuntime="MyBatis3Simple">
        <jdbcConnection driverClass="org.h2.Driver"
                        connectionURL="jdbc:h2:~/h2database/HospitalDB4;AUTO_SERVER=TRUE"
                        userId="sa"
                        password="sa">
        </jdbcConnection>

        <javaModelGenerator
                targetPackage="lt.vu.uc.mybatis.model"
                targetProject="MAVEN" />

        <sqlMapGenerator targetPackage="lt.vu.uc.mybatis.xml"
                         targetProject="MAVEN" />
        <javaClientGenerator type="XMLMAPPER"
                             targetPackage="lt.vu.uc.mybatis.dao"
                             targetProject="MAVEN" />

        <table schema="public" tableName="doctor">
            <generatedKey column="id" identity="true" sqlStatement="JDBC" />
        </table>
        <table schema="public" tableName="sector">
            <generatedKey column="id" identity="true" sqlStatement="JDBC" />
        </table>
    </context>
</generatorConfiguration>